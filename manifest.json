{
	"name": "CLKD",
	"description": "Supercharged Links & Pages.",
	"version": "0.0.1",
	"manifest_version": 3,
	"homepage_url": "https://clkd.in",
	"author": {
		"email": "akshitsethi@gmail.com"
	},
	"side_panel": {
		"default_path": "main.html"
	},
	"background": {
		"service_worker": "service-worker.js"
	},
	"content_scripts": [
		{
			"matches": ["http://localhost:8788/page/preview?*"],
			"js": ["assets/dist/preview.js"],
			"all_frames": true
		}
	],
	"permissions": [
		"tabs",
		"storage",
		"identity",
		"sidePanel",
		"downloads",
		"unlimitedStorage"
	],
	"icons": {
		"16": "assets/images/icon-16.png",
		"32": "assets/images/icon-32.png",
		"48": "assets/images/icon-48.png",
		"128": "assets/images/icon-128.png"
	},
	"web_accessible_resources": [
		{
			"resources": ["assets/images/night.png", "assets/images/spinner.svg"],
			"matches": ["http://localhost:8788/*"]
		}
	],
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self';"
	},
	"key": "",
	"oauth2": {
		"client_id": "485256031642-tlda5lpocaivq9qoqtk62lev62382gv9.apps.googleusercontent.com",
		"scopes": [
			"https://www.googleapis.com/auth/userinfo.email",
			"https://www.googleapis.com/auth/userinfo.profile",
			"openid"
		]
	}
}
